#Maze 3 solution


def turn_right():
    turn_left()
    turn_left()
    turn_left()


while not at_goal():
    if wall_in_front():
        turn_right()
    if not wall_in_front():
        move()
        if right_is_clear():
            turn_right()
        if not right_is_clear():
            turn_left()
    else:
        turn_left()
        turn_left()
